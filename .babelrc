{
  "presets": [
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ],
    [
      "@babel/env",
      {
        "modules": false,
        "targets": { "node": "12" }
      }
    ]
  ],
  "plugins": [
    "@babel/syntax-class-properties",
    "@babel/transform-modules-commonjs",
    "@babel/plugin-proposal-do-expressions"
  ]
}
